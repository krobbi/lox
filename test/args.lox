// Test command line argument extensions.

var argc = __argc();
print "Arguments:";
print argc;

for (var i = 0; i < argc; i = i + 1) {
	print "Argument - '" + __argv(i) + "'";
}

print "Expect 'nil's:";
print __argc(0); // Too many arguments.
print __argv(); // Not enough arguments.
print __argv(0, 1); // Too many arguments.
print __argv(-1); // Below range.
print __argv(argc); // Above range.
print __argv(nil); // Unexpected nil.
print __argv(true); // Unexpected true.
print __argv(false); // Unexpected false.
print __argv("0"); // Unexpected object.

print "Expect 'true's:";
print __argv(0.01) == __argv(0); // Index is rounded.
print __argv(0.99) == __argv(0); // Index is rounded down.
print __argv(argc - 0.01) == __argv(argc - 1); // Index is rounded into range.
