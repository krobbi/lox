// A node of a list.
class ListNode {
	// Initialize the list node from its element and connecting list nodes.
	init(elem, prev, next) {
		// The list node's element.
		this.elem = elem;
		
		// The list node's previous list node.
		this.prev = prev;
		
		// The list node's next list node.
		this.next = next;
	}
}
